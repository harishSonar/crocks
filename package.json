{
  "name": "amdocs-crocodile",
  "version": "1.0.0",
  "description": "Digital line Hackathon 2019 project for UI design and code generator",
  "main": "src/index.js",
  "module": "src/index.js",
  "jsnext:amdocs": "index.js",
  "scripts": {
    "clean:template": "rimraf src/templates ",
    "clean:build": "rimraf dist/ lib/ ",
    "clean:test": "rimraf test-reports/ .tmp/ .nyc_output/",
    "eslint": "eslint \"./src/**/*.js\"",
    "comp:es6": "cross-env BABEL_ENV=comp-es6 babel src/ --out-dir lib/ --copy-files",
    "dev": "npm-run-all --serial clean:build eslint dev:webpackServer -- --watch",
    "dev:webpackServer": "cross-env NODE_ENV=development webpack-dev-server --progress --config ./build/webpack.dev.js",
    "start": "npm-run-all --serial dev",
    "build:prod": "cross-env NODE_ENV=production webpack --config ./build/webpack.prod.js --progress --optimize-minimize",
    "build": "npm-run-all --serial clean:build eslint --parallel build:prod",
    "gen-source-es5": "node lib/generators/index.js",
    "gen-source-transpile" : "babel ./src/generators --out-dir lib/ --copy-file && webpack --config ./build/webpack.dev.js",
    "gen-source": "npm-run-all --serial clean:template comp:es6 gen-source-es5",
    "gen-design": "npm-run-all --serial clean:build eslint design:webpackServer -- --watch",
    "design:webpackServer": "cross-env NODE_ENV=development webpack-dev-server --progress --config ./build/webpack.design.js"
  },
  "author": "Amdocs Inc.",
  "license": "ISC",
  "dependencies": {
    "@types/fs-extra": "^5.0.5",
    "fs": "0.0.1-security",
    "fs-extra": "^7.0.1",
    "prop-types": "~15.5.10",
    "react": "~15.6.0",
    "react-dom": "~15.6.0",
    "react-intl": "~2.3.0",
    "react-redux": "~5.0.6"
  },
  "devDependencies": {
    "babel-cli": "~6.24.0",
    "babel-eslint": "~8.2.1",
    "babel-loader": "~7.1.0",
    "babel-plugin-transform-class-properties": "~6.24.0",
    "babel-plugin-transform-decorators-legacy": "~1.3.4",
    "babel-plugin-transform-object-rest-spread": "~6.23.0",
    "babel-plugin-transform-rename-import": "~2.1.0",
    "babel-plugin-transform-runtime": "~6.7.0",
    "babel-preset-env": "~1.5.0",
    "babel-preset-react": "~6.24.0",
    "cross-env": "~5.0.0",
    "concurrently": "^4.1.0",
    "copy-webpack-plugin": "~4.6.0",
    "eslint": "~4.17.0",
    "eslint-config-airbnb": "~15.0.0",
    "eslint-import-resolver-webpack": "~0.8.3",
    "eslint-loader": "~1.8.0",
    "eslint-plugin-import": "~2.3.0",
    "eslint-plugin-jsx-a11y": "~5.0.3",
    "eslint-plugin-react": "~7.1.0",
    "glob": "~7.1.3",
    "html-webpack-plugin": "~3.2.0",
    "jsdom": "~10.1.0",
    "jsdom-global": "~3.0.2",
    "json-loader": "~0.5.7",
    "mermaid": "8.0.0",
    "mock-local-storage": "~1.0.5",
    "ncp": "~2.0.0",
    "node-sass": "~4.11.0",
    "nodemon": "^1.18.9",
    "npm-run-all": "~4.0.0",
    "nyc": "~13.0.0",
    "rimraf": "~2.6.0",
    "webpack": "~4.29.6",
    "webpack-cli": "3.3.0",
    "webpack-dev-server": "~3.2.1",
    "webpack-merge": "~4.1.1",
    "write-file-webpack-plugin": "~4.2.0"
  },
  "publishConfig": {},
  "engines": {
    "node": ">=5.0.0"
  }
}
